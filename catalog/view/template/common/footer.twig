{{ callback }}
<footer>
  <div class="container">
    <div class="row gy-4">
      <div class="col-lg-3">
        <div class="footer_logo">
          <a href="#">
            <img src="/image/catalog/logo/logo-dark.svg" class="img-fluid" alt="logo">
          </a>
        </div>
        <div class="footer_info">
          <p>AK Clinics- India most trusted & recommended Hair, Skin, Cosmetic Surgery Clinics in India run by Dr. Kapil Dua & Dr. Aman Dua.</p>
        </div>
      </div>
      <div class="col-lg-2">
        <h5>Explore</h5>
        {% if informations %}
        <ul class="list-unstyled">
          {% for information in informations %}
            <li>
              <a href="{{ information['href'] }}"{% if information['external'] %} target="_blank"{% endif %}>{{ information['title'] }}</a>
            </li>
          {% endfor %}
        </ul>
        {% endif %}
      </div>
      <div class="col-lg-4">
        <div class="footer_contact">
          <h5>Contact</h5>
          <a href="tel:{{ telephone }}">{{ telephone }}</a>
          <a href="mailto:{{ email }}">{{ email }}</a>
          <p>{{ address }}</p>
        </div>
      </div>
      <div class="col-lg-3">
        <div class="newsletter">
          {{ subscribe }}
          <h4>Follow Us On</h4>
          <ul class="list-inline">
            <li class="list-inline-item">
              <a href="{{ FB_LINK }}" target="_blank"><i class="bi bi-facebook"></i></a>
            </li>
            <li class="list-inline-item">
              <a href="{{ LI_LINK }}" target="_blank"><i class="bi bi-linkedin"></i></a>
            </li>
            <li class="list-inline-item">
              <a href="{{ IG_LINK }}" target="_blank"><i class="bi bi-instagram"></i></a>
            </li>
            <li class="list-inline-item">
              <a href="{{ YT_LINK }}" target="_blank"><i class="bi bi-youtube"></i></a>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</footer>
{{ cookie }}
<script src="{{ bootstrap }}" type="text/javascript"></script>
{% for script in scripts %}
  <script src="{{ script.href }}" type="text/javascript"></script>
{% endfor %}
